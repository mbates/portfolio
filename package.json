{
  "name": "portfolio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "deploy:gateway": "serverless --aws-profile=bates --config ./serverless/gateway.yml deploy",
    "remove:gateway": "serverless --aws-profile=bates --config ./serverless/gateway.yml remove",
    "deploy:lambda": "serverless --aws-profile=bates --config ./serverless/lambda.yml deploy",
    "remove:lambda": "serverless --aws-profile=bates --config ./serverless/lambda.yml remove"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.11",
    "@types/node": "24.2.1",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.7",
    "axios": "1.11.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-router-dom": "7.9.4",
    "react-terminal": "1.4.5",
    "tailwindcss": "4.1.8"
  },
  "devDependencies": {
    "@aws-sdk/client-ses": "^3.965.0",
    "@eslint/js": "9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "9.32.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.4.0",
    "jsdom": "^27.4.0",
    "sass": "1.97.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "vite": "7.3.1",
    "vitest": "^3.2.4"
  }
}
