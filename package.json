{
  "name": "portfolio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:gateway": "serverless --aws-profile=bates --config ./src/serverless/gateway.yml deploy",
    "remove:gateway": "serverless --aws-profile=bates --config ./src/serverless/gateway.yml remove",
    "deploy:lambda": "serverless --aws-profile=bates --config ./src/serverless/lambda.yml deploy",
    "remove:lambda": "serverless --aws-profile=bates --config ./src/serverless/lambda.yml remove"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.4",
    "@types/node": "22.15.29",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.5.3",
    "react-terminal": "1.4.5",
    "tailwindcss": "4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "@vitejs/plugin-react": "4.5.0",
    "eslint": "9.27.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.2.0",
    "sass": "1.89.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.0",
    "vite": "6.3.5"
  }
}
